    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "postgrator": "^4.0.0",
    "treeize": "^2.1.2",
    "url": "^0.11.0",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
